{
  "name": "plotly-mock-viewer",
  "version": "1.0.11",
  "description": "A budo-based live-reloading standalone viewer for plotly mocks",
  "main": "index.js",
  "scripts": {
    "start": "node cli.js",
    "build": "browserify lib/production.js -t es2040 -t brfs | indexhtmlify | metadataify | htmlinjectscript \"https://cdn.plot.ly/plotly-latest.js\" | github-cornerify > index.html",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "bin": {
    "plotly-mock-viewer": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rreusser/plotly-mock-viewer.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "brfs": "^1.4.3",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "es2040": "^1.2.5",
    "h": "^0.1.0",
    "insert-css": "^2.0.0",
    "minimist": "^1.2.0",
    "pad-left": "^2.1.0",
    "pkg-up": "^1.0.0",
    "simple-html-index": "^1.5.0",
    "string-to-stream": "^1.1.0",
    "html-inject-script": "^1.1.0"
  },
  "devDependencies": {
    "github-cornerify": "^1.0.7",
    "indexhtmlify": "^1.3.1",
    "metadataify": "^1.0.3",
    "plotly.js": "^1.25.0"
  }
}
